{
    "name": "nautic/transifex",
    "description": "The Transifex API Package is a wrapper of the Transifex API available for PHP developers",
    "keywords": ["php", "transifex"],
    "homepage": "https://github.com/mautic/Transifex-API",
    "license": "GPL-2.0+",
    "abandoned": true,
    "authors": [
        {
            "name": "Michael Babker",
            "homepage": "https://www.babdev.com"
        }
    ],
    "require": {
        "php": "~7.2",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.4.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "phpunit/phpunit": "^8.4.3",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Mautic\\Transifex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mautic\\Transifex\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
